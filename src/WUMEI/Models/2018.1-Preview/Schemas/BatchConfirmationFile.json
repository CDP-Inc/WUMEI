                "functionReturnText": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "minLength": 1,
                    "maxLength": 50,
                    "pattern": "[a-zA-Z0-9\\s]+",
                    "errorMessage": ""
                },
                "originalLineNumber": {
                    "type": "integer",
                    "minLength": 0,
                    "maxLength": 9999999,
                    "errorMessage": "0218"
                },
                "recordSequenceNumber": {
                    "type": "integer",
                    "minLength": 1,
                    "maxLength": 999999,
                    "errorMessage": "0064"
                }
            },
            "required": [
                "functionReturnCode",
                "originalLineNumber",
                "recordSequenceNumber"
            ]
