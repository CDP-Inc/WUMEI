                    "maximum": 999999999,
                    "errorMessage": "0139"
                },
                "transmissionDateTime": {
                    "type": "string",
                    "pattern": "^([0-9]{4}-[0-9]{2}-[0-9]{2}T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\\.[0-9]{1,3})?Z)$",
                    "errorMessage": "0142"
                },
                "fileName": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "maxLength": 45,
                    "errorMessage": "0365"
                },
                "fileSequenceNumber": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 999999,
                    "errorMessage": "0176"
                },
                "fileFormatVersion": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 99,
                    "errorMessage": "0366"
                },
                "fileTypeCode": {
                    "type": "integer",
                    "minimum": 9400,
                    "maximum": 9400,
                    "errorMessage": "0194"
                },
                "originatorOfFile": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 99999999999,
                    "errorMessage": "0367"
                },
                "receivingInstitutionIdentificationCode": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 99999999999,
                    "errorMessage": "0242"
                },
                "typeCode": {
                    "type": [
                        "string",
                        "null"
                    ],
                    "pattern": "U",
                    "errorMessage": "0347"
                },
                "wicStateAgencyId": {
                    "type": "string",
                    "minLength": 1,
                    "maxLength": 3,
                    "pattern": "\\d+",
                    "errorMessage": "0273"
                },
                "originalMessageorFileTypeCode": {
                    "type": "integer",
                    "minimum": 1000,
                    "maximum": 9700,
                    "errorMessage": ""
                },
                "rejectedLineCount": {
                    "type": "integer",
                    "minimum": 0,
                    "maximum": 9999999,
                    "errorMessage": "0244"
                }
            },
            "required": [
                "detailRecordsCount",
                "transmissionDateTime",
                "fileSequenceNumber",
                "fileFormatVersion",
                "fileTypeCode",
                "originatorOfFile",
                "receivingInstitutionIdentificationCode",
                "TypeCode",
                "wicStateAgencyId",
                "originalMessageOrFileType",
                "rejectedLineCount"
