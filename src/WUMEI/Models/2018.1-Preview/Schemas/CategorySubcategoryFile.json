{
  "definitions": {
    "batchHeader": {
      "type": "object",
      "properties": {
        "detailRecordsCount": {
          "type": "integer",
          "minimum": 0,
          "maximum": 999999999,
          "errorMessage": "0139"
        },
        "transmissionDateTime": {
          "type": "string",
          "pattern": "^([0-9]{4}-[0-9]{2}-[0-9]{2}T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\\.[0-9]{1,3})?Z)$",
          "errorMessage": "0142"
        },
        "fileName": {
          "type": [
            "string",
            "null"
          ],
          "maxLength": 45,
          "pattern": "^([A-Za-z0-9#$%&'()*+,\\-./:;<>=>?@[\\\\\\]^{|}~]+)$",
          "errorMessage": "0365"
        },
        "fileSequenceNumber": {
          "type": "integer",
          "minimum": 0,
          "maximum": 999999,
          "errorMessage": "0176"
        },
        "fileFormatVersion": {
          "type": "integer",
          "minimum": 0,
          "maximum": 99,
          "errorMessage": "0366"
        },
        "fileTypeCode": {
          "type": "integer",
          "minimum": 9400,
          "maximum": 9400,
          "errorMessage": "0191"
        },
        "originatorOfFile": {
          "type": "integer",
          "minimum": 0,
          "maximum": 99999999999,
          "errorMessage": "0367"
        },
        "receivingInstitutionIdentificationCode": {
          "type": "integer",
          "minimum": 0,
          "maximum": 99999999999,
          "errorMessage": "0242"
        },
        "typeCode": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^(U)$",
          "errorMessage": "0347"
        },
        "wicStateAgencyId": {
          "type": "integer",
          "minimum": 0,
          "maximum": 999,
          "errorMessage": "0273"
        }
      },
      "required": [
        "detailRecordsCount",
        "transmissionDateTime",
        "fileSequenceNumber",
        "fileFormatVersion",
        "fileTypeCode",
        "originatorOfFile",
        "receivingInstitutionIdentificationCode",
        "wicStateAgencyId"
      ]
    },
    "batchRecordHeader": {
      "type": "object",
      "properties": {
        "recordSequenceNumber": {
          "type": "integer",
          "minimum": 1,
          "maximum": 999999,
          "errorMessage": "0064"
        },
        "username": {
          "type": [
            "string",
            "null"
          ],
          "maxLength": 50,
          "pattern": "^([A-Za-z0-9#$%&'()*+,\\-./:;<>=>?@[\\\\\\]^{|}~]+)$",
          "errorMessage": "0266"
        },
        "workstationId": {
          "type": [
            "string",
            "null"
          ],
          "maxLength": 20,
          "pattern": "^([A-Za-z0-9]+)$",
          "errorMessage": "0279"
        }
      },
      "required": [
        "recordSequenceNumber"
      ]
    },
    "categorySubcategoryRecord": {
      "type": "object",
      "properties": {
        "recordHeader": {
          "$ref": "#/definitions/batchRecordHeader",
          "errorMessage": "0064"
        },
        "actionCode": {
            "type": "integer",
            "minimum": 2,
            "maximum": 2,
            "errorMessage": "0071"
        },
        "benefitUnitDescription": {
          "type": "string",
          "minLength": 1,
          "maxLength": 6,
          "errorMessage": "0114"
        },
        "categoryCode": {
          "type": "integer",
          "minimum": 0,
          "maximum": 99,
          "errorMessage": "0131"
        },
        "categoryLongDescription": {
          "type": [
            "string",
            "null"
          ],
          "minLength": 1,
          "maxLength": 50,
          "pattern": "^([A-Za-z0-9\\s#$%&'()*+,\\-./:;<>=>?@[\\\\\\]^{|}~]+)$",
          "errorMessage": "0134"
        },
        "categoryShortDescription": {
          "type": "string",
          "minLength": 1,
          "maxLength": 20,
          "pattern": "^([A-Za-z0-9\\s#$%&'()*+,\\-./:;<>=>?@[\\\\\\]^{|}~]+)$",
          "errorMessage": "0135"
        },
        "beginCategoryDate": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^([0-9]{4}-[0-9]{2}-[0-9]{2})$",
          "errorMessage": "0150"
        },
        "beginSubcategoryDate": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^([0-9]{4}-[0-9]{2}-[0-9]{2})$",
          "errorMessage": "0152"
        },
        "endCategoryDate": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^([0-9]{4}-[0-9]{2}-[0-9]{2})$",
          "errorMessage": "0162"
        },
        "endSubcategoryDate": {
          "type": [
            "string",
            "null"
          ],
          "pattern": "^([0-9]{4}-[0-9]{2}-[0-9]{2})$",
          "errorMessage": "0166"
        },
        "subcategoryCode": {
          "type": "integer",
          "minimum": 0,
          "maximum": 999,
          "errorMessage": "0248"
        },
        "subcategoryLongDescription": {
          "type": [
            "string",
            "null"
          ],
          "minLength": 1,
          "maxLength": 50,
          "pattern": "^([A-Za-z0-9\\s#$%&'()*+,\\-./:;<>=>?@[\\\\\\]^{|}~]+)$",
          "errorMessage": "0251"
        },
        "subcategoryShortDescription": {
          "type": [
            "string",
            "null"
          ],
          "minLength": 1,
          "maxLength": 24,
          "pattern": "^([A-Za-z0-9\\s#$%&'()*+,\\-./:;<>=>?@[\\\\\\]^{|}~]+)$",
          "errorMessage": "0252"
        }
      },
      "required": [
        "recordHeader",
        "actionCode",
        "benefitUnitDescription",
        "categoryCode",
        "categoryShortDescription",
        "subcategoryCode",
        "subcategoryShortDescription"
      ]
    }
  },
  "type": "object",
  "properties": {
    "header": {
      "$ref": "#/definitions/batchHeader",
      "errorMessage": "0139"
    },
    "records": {
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "#/definitions/categorySubcategoryRecord"
      }
    }
  },
  "required": [
    "header"
  ]
}